                <lightning:layoutItem class="slds-align_absolute-center" size="12" smallDeviceSize="12" mediumDeviceSize="3" largeDeviceSize="4" padding="around-small">
                    <lightning:carousel disableAutoRefresh="false" disableAutoScroll="false">
